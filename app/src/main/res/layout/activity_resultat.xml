<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".resultat">

    <TextView
        android:id="@+id/origine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/marque"
        app:layout_constraintTop_toBottomOf="@+id/marque" />

    <TextView
        android:id="@+id/nom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="46dp"
        android:layout_marginTop="83dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/Aff_nom"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/marque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/nom"
        app:layout_constraintTop_toBottomOf="@+id/nom" />

    <TextView
        android:id="@+id/empreinteC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/origine"
        app:layout_constraintTop_toBottomOf="@+id/origine" />

    <TextView
        android:id="@+id/note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:scrollbarSize="4dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/empreinteC"
        app:layout_constraintTop_toBottomOf="@+id/empreinteC" />

    <TextView
        android:id="@+id/Aff_eC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:text="Empreinte Carbone :"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/empreinteC" />

    <TextView
        android:id="@+id/Aff_note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:text="Note : "
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/Aff_eC"
        app:layout_constraintTop_toBottomOf="@+id/Aff_eC" />

    <TextView
        android:id="@+id/Aff_o"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:text="Origine : "
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/Aff_eC"
        app:layout_constraintEnd_toEndOf="@+id/Aff_eC" />

    <TextView
        android:id="@+id/Aff_m"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:text="Marque :"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/Aff_o"
        app:layout_constraintEnd_toEndOf="@+id/Aff_o" />

    <TextView
        android:id="@+id/Aff_nom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:text="Produit : "
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/nom"
        app:layout_constraintStart_toStartOf="@+id/Aff_m" />
</androidx.constraintlayout.widget.ConstraintLayout>